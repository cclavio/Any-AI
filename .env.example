# Server port (default: 3000)
PORT=3000

# App package name — must match the MentraOS Developer Console exactly
PACKAGE_NAME=org.yourname.appname

# MentraOS developer API key from the Developer Console
MENTRAOS_API_KEY=your_mentraos_api_key

# Supabase Postgres connection string (pooler/transaction mode)
DATABASE_URL=postgres://postgres.YOUR_PROJECT:YOUR_PASSWORD@aws-0-us-east-1.pooler.supabase.com:6543/postgres

# Cookie signing secret (generate with: openssl rand -hex 32)
COOKIE_SECRET=your_cookie_secret

# Public URL for serving static assets (audio cues, etc.)
PUBLIC_URL=http://localhost:3000

# Jina AI API key — used for web search tool
JINA_API_KEY=your_jina_api_key

# Google Maps API key — used for reverse geocoding and timezone detection
# Enable these APIs in Google Cloud Console: Geocoding API, Time Zone API
GOOGLE_MAPS_API_KEY=your_google_maps_api_key
