{
  "name": "Any AI",
  "description": "A multi-provider, bring-your-own-key AI assistant for MentraOS smart glasses. Forked from Mentra AI 2 app",
  "version": "0.8.0",
  "onboardingInstructions": "1. Add [any] AI as a background App. Make sure you remove the Mentra AI app from background apps so you don't have Any AI and Mentra AI running simultaneously.\n2. Tap Background Apps, and then the \"right\" arrow for the [any] AI app to configure your AI provider and model.\n3. Choose your provider and models for LLM and Vision (OpenAI, Anthropic, Google Gemini are all supported).\n4. Add your API key(s).\n5. Enjoy!",
  "publicUrl": "https://any-ai-production.up.railway.app",
  "logoURL": "https://mentra-store-cdn.mentraglass.com/mini_app_assets/com.clavionlabs.anyai/1771730415158-any-ai-logo-2.png",
  "appType": "background",
  "permissions": [
    {
      "type": "MICROPHONE",
      "description": "Access to microphone for voice input and audio processing"
    }
  ],
  "settings": [
    {
      "type": "group",
      "key": "group_personalization",
      "label": "Personalization",
      "title": "Personalization"
    },
    {
      "type": "text",
      "key": "agent_name",
      "label": "Assistant Name",
      "defaultValue": "Any AI"
    },
    {
      "type": "text",
      "key": "wake_word",
      "label": "Wake Word",
      "defaultValue": "Hey Jarvis"
    },
    {
      "type": "group",
      "key": "group_llm",
      "label": "LLM Provider",
      "title": "LLM Provider"
    },
    {
      "type": "select",
      "key": "llm_provider",
      "label": "Provider",
      "options": [
        { "label": "OpenAI", "value": "openai" },
        { "label": "Anthropic", "value": "anthropic" },
        { "label": "Google", "value": "google" }
      ],
      "defaultValue": "openai"
    },
    {
      "type": "select_with_search",
      "key": "llm_model",
      "label": "Model",
